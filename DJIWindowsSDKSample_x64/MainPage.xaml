        </NavigationView>
        <Image x:Name="logo" Source="/Assets/Dermdronelogov4.png" RenderTransformOrigin="0.5,0.888" Margin="334,0,603,670" Stretch="Uniform" RequestedTheme="Dark" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>


    </Grid>
